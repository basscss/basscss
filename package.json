{
  "name": "basscss",
  "version": "5.2.1",
  "description": "Low-level CSS toolkit",
  "style": "src/basscss.css",
  "scripts": {
    "test": "mocha test",
    "css": "node scripts/css",
    "watch:css": "watch 'npm run css' ./src",
    "sassify": "node scripts/sassify",
    "watch:sassify": "watch 'npm run sassify' ./src",
    "html": "node scripts/autobass",
    "watch:html": "watch 'npm run html' ./docs/src",
    "bower-bump": "node scripts/bower-bump",
    "s3": "node scripts/s3",
    "gzip": "gzip -c css/basscss.min.css > css/basscss.min.css.gz",
    "zip": "node scripts/zip",
    "customizer:data": "node scripts/customizer",
    "customizer:js": "browserify docs/src/docs/customize/app.js -t uglifyify -o docs/customize/app.min.js",
    "release": "npm run css && npm run zip && npm run sassify && npm run bower-bump && npm run customizer:data && npm run customizer:js",
    "serve": "http-server -p 8000",
    "start": "npm run watch:css & npm run watch:sassify & npm run watch:html & npm run serve"
  },
